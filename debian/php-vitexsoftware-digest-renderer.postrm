#!/bin/bash
set -e

case "$1" in
    remove|purge)
        # Remove from alternatives system
        if [ -x /usr/bin/update-alternatives ]; then
            update-alternatives --quiet --remove autoload-php \
                /usr/share/php/VitexSoftware/DigestRenderer/vendor/autoload.php || true
        fi
        ;;
esac

#DEBHELPER#

exit 0